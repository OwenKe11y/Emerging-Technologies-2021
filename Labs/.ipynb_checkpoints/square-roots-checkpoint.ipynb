   "source": [
    "# Loop through values (i) from 0 to x-1.\n",
    "for i in range(x - 1):\n",
    "    # Check if i squared is equal to x.\n",
    "    if (i**2) - x == 0:\n",
    "        # Print the square root.\n",
    "        print(i)\n",
    "        # Finish.\n",
    "        break"
   ]
